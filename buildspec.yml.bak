version: 0.2
phases:
  install:
    commands:
      - sam package --template-file template.yaml --output-template-file package.yaml --s3-bucket samtestpackage-testlambda-app
      - aws cloudformation package --template-file template.yaml --s3-bucket samtestpackage-testlambda-app --output-template-file package.yaml
    
artifacts:
  type: zip
  files:
    - template.yaml
    - package.yaml